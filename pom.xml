<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    
    <parent>
        <groupId>fish.focus.uvms.maven</groupId>
        <artifactId>uvms-pom</artifactId>
        <relativePath />
        <version>1.19</version>
    </parent>
    
    <groupId>se.havochvatten.vessel.proxy.cache</groupId>
    <artifactId>hav-vessel-proxy-cache</artifactId>
    <version>3.0.2</version>
    
    <packaging>pom</packaging>
    
    
    <name>${project.artifactId}</name>
    
    <properties>
        <maven-release-plugin>2.5.3</maven-release-plugin>

        <asset.model.version>6.0.15</asset.model.version>
        <uvms.common.version>3.0.29</uvms.common.version>

        <vessel.client.version>2.1.20</vessel.client.version>
        <vessel.comp.client.version>2.0.3</vessel.comp.client.version>
        <notification.client.version>4.0.31</notification.client.version>
        <equipment.client.version>1.0.2</equipment.client.version>

        <project.scm.id>github</project.scm.id>
    	<release.branch.name>uvms-3.0.0-boba-fett</release.branch.name>
    </properties>
    
    <dependencies>
        <dependency>
            <groupId>javax</groupId>
            <artifactId>javaee-api</artifactId>
            <version>7.0</version>
            <scope>provided</scope>
        </dependency>

		<dependency>
			<groupId>fish.focus.uvms.maven</groupId>
			<artifactId>uvms-pom-logging-deps</artifactId>
			<version>1.19</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>fish.focus.uvms.maven</groupId>
			<artifactId>uvms-pom-test-deps</artifactId>
			<version>1.19</version>
			<type>pom</type>
		</dependency>
    </dependencies>

    <scm>
        <connection>scm:git:https://github.com/UnionVMS/UVMS-AssetModule-PROXY-HAV-CACHE.git</connection>
      <tag>hav-vessel-proxy-cache-3.0.2</tag>
  </scm>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-release-plugin</artifactId>
                <version>${maven-release-plugin}</version>
                <dependencies>
                    <dependency>
                        <groupId>org.apache.maven.scm</groupId>
                        <artifactId>maven-scm-provider-gitexe</artifactId>
                        <version>1.8.1</version>
                    </dependency>
                </dependencies>
                <configuration>
                    <checkModificationExcludes>
                        <checkModificationExclude>src/main/java/eu/europa/ec/fisheries/wsdl</checkModificationExclude>
                    </checkModificationExcludes>
                </configuration>
            </plugin>
        </plugins>
    </build>
    
  <modules>
    <module>module</module>
    <module>service</module>
  </modules>
</project>